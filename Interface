    }                                          

    private void jPanel1MouseReleased(java.awt.event.MouseEvent evt) {                                      
        // TODO add your handling code here:

        if (evt.isPopupTrigger()) {
            JPopmenu01.show(this, evt.getX(), evt.getY());
        }


    }                                     

    private void MODIFCARActionPerformed(java.awt.event.ActionEvent evt) {                                         

        if (JTableFrete.getSelectedRow() != -1) {

            Fretamento f = new Fretamento();
            FretamentoDAO fdao = new FretamentoDAO();

            f.setData(txtData.getText());
            f.setOrigem(txtOrigem.getText());
            f.setDestino(txtDestino.getText());
            f.setValorTonelada(new BigDecimal(txtValorTonelada.getText()));
            f.setToneladaCarregada(new BigDecimal(txtToneladaCarregada.getText()));
            //fdao.update();

            JOptionPane.showMessageDialog(null, "Registro atualizado na tabela!");
            try {
                fdao.update(f);
            } catch (ParseException ex) {
                Logger.getLogger(TelaCalculo.class.getName()).log(Level.SEVERE, null, ex);
            }

        }
    }                                        

    private void MODIFCARMouseReleased(java.awt.event.MouseEvent evt) {                                       

        // Atualiza os valores na JTable
        JTableFrete.setValueAt(txtData.getText(), JTableFrete.getSelectedRow(), 0);
        JTableFrete.setValueAt(txtOrigem.getText(), JTableFrete.getSelectedRow(), 1);
        JTableFrete.setValueAt(txtDestino.getText(), JTableFrete.getSelectedRow(), 2);
        JTableFrete.setValueAt(txtValorTonelada.getText(), JTableFrete.getSelectedRow(), 3);
        JTableFrete.setValueAt(txtToneladaCarregada.getText(), JTableFrete.getSelectedRow(), 4);

        // Exibir uma mensagem de sucesso
        JOptionPane.showMessageDialog(null, "Registro atualizado na tabela!");


    }                                      

