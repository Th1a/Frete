    }                                                

    private void EXCLUIRActionPerformed(java.awt.event.ActionEvent evt) {                                        
        // TODO add your handling code here:
        if (JTableFrete.getSelectedRow() != -1) {

            DefaultTableModel dtm = (DefaultTableModel) JTableFrete.getModel();
            dtm.removeRow(JTableFrete.getSelectedRow());

        } else {
            JOptionPane.showMessageDialog(null, " Selecione para excluir");
        }

    }                                       

